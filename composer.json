{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "doctrine/dbal": "^2.6",
    "fedeisas/laravel-mail-css-inliner": "^2.2",
    "fideloper/proxy": "~3.3",
    "laravel/framework": "5.5.*",
    "laravel/passport": "^4.0",
    "laravel/tinker": "~1.0",
    "paypal/rest-api-sdk-php": "*",
    "ramsey/uuid": "^3.7",
    "spatie/laravel-activitylog": "^2.5",
    "spatie/laravel-medialibrary": "6.0.0",
    "spatie/laravel-model-status": "^1.0",
    "spatie/laravel-permission": "^2.8"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "classmap": ["database/seeds", "database/factories"],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": ["@php artisan key:generate"],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
